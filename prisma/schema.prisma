//output   = "../../node_modules/.prisma/client"

generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Dev_project {
  id        Int       @id @default(autoincrement())
  name      String    @db.VarChar(100)
  desc      String    @db.VarChar(100)
  create_by String    @db.VarChar(20)
  update_by String?   @db.VarChar(20)
  create_at DateTime?
  update_at DateTime?

  @@map("dev_projects")
}

model DevTableColumn {
  id         Int    @id @default(autoincrement())
  table_id   Int
  table_name String @db.VarChar(100)
  name       String @db.VarChar(100)
  desc       String @db.VarChar(255)
  type       String @db.VarChar(20) //varchar,numeric,date,time dll
  length     Int    @default(1)
  is_id      String @db.VarChar(2) //Yes,No
  is_null    String @db.VarChar(2) //Yes,NO
  is_uniq    String @db.VarChar(2) //Yesy, NO
  default    String @db.VarChar(20)

  @@map("dev_tables_column")
}

model User {
  username  String    @id @db.VarChar(100)
  password  String    @db.VarChar(100)
  name      String    @db.VarChar(100)
  token     String?   @db.VarChar(100)
  create_by String    @default("admin") @db.VarChar(20)
  update_by String?   @db.VarChar(20)
  create_at DateTime?
  update_at DateTime?
  // contacts  Contact[]
  status    String?   @db.VarChar(20)
  email     String?   @db.VarChar(250)
  group     String?   @db.VarChar(100)
  expired   DateTime?

  @@map("users")
}

model Project {
  id         Int       @id @default(autoincrement())
  name       String    @db.VarChar(100)
  desc       String    @db.VarChar(100)
  project_id Int
  create_by  String    @db.VarChar(20)
  update_by  String?   @db.VarChar(20)
  create_at  DateTime?
  update_at  DateTime?

  @@map("projects")
}

model Dev_tablex {
  id         Int       @id @default(autoincrement())
  name       String    @db.VarChar(100) ///tolong di cek dulu pada kolom is_nulll yaa..... perbaiki
  desc       String    @db.VarChar(100)
  create_by  String    @db.VarChar(20)
  update_by  String?   @db.VarChar(20)
  create_at  DateTime?
  update_at  DateTime?
  project_id Int

  @@map("dev_tablexs")
}

//Create Schema
//schema.prisma

model DevTableKolom {
  id         Int       @id @default(autoincrement())
  name       String    @db.VarChar(100)
  table_id   Int
  table_name String    @db.VarChar(100)
  desc       String    @db.VarChar(100)
  length     Int
  is_id      String    @db.VarChar(100)
  is_null    String    @db.VarChar(100)
  is_uniq    String    @db.VarChar(100)
  default    String    @db.VarChar(100)
  type       String    @db.VarChar(100)
  create_by  String    @db.VarChar(20)
  update_by  String?   @db.VarChar(20)
  create_at  DateTime?
  update_at  DateTime?

  @@map("dev_table_koloms")
}

//Create Schema DevDirektori

model DevDirektori {
  id        Int       @id @default(autoincrement())
  username  String    @unique @db.VarChar(100)
  frontend  String?   @db.VarChar(250)
  backend   String?   @db.VarChar(250)
  create_by String    @db.VarChar(20)
  update_by String?   @db.VarChar(20)
  create_at DateTime?
  update_at DateTime?

  @@map("dev_direktoris")
}

//Create Schema Seq
model Seq {
  id           Int       @id @default(autoincrement())
  kode         String    @db.VarChar(30)
  tahun        Int
  last_squence Int       @default(0)
  desc         String?   @db.VarChar(30)
  create_by    String    @db.VarChar(20)
  update_by    String?   @db.VarChar(20)
  create_at    DateTime?
  update_at    DateTime?

  @@map("seqs")
}

//Create Schema Group
model Group {
  id        Int       @id @default(autoincrement())
  name      String    @unique @db.VarChar(100)
  desc      String?   @db.VarChar(250)
  pic       String?   @db.VarChar(250)
  status    String    @db.VarChar(3)
  create_by String    @db.VarChar(20)
  update_by String?   @db.VarChar(20)
  create_at DateTime?
  update_at DateTime?

  @@map("groups")
}

//Create Schema UserProject
model UserProject {
  id         String    @id @default(uuid())
  project_id Int
  username   String?   @db.VarChar(100)
  status     String?   @db.VarChar(20)
  create_by  String    @db.VarChar(20)
  update_by  String?   @db.VarChar(20)
  create_at  DateTime?
  update_at  DateTime?

  @@map("user_projects")
}

//Create Schema TableCoba
model TableCoba {
  id        String    @id @default(uuid())
  name      String?   @db.VarChar(30)
  kode      String    @unique @db.VarChar(20)
  create_by String    @db.VarChar(20)
  update_by String?   @db.VarChar(20)
  create_at DateTime?
  update_at DateTime?

  @@map("table_cobas")
}

